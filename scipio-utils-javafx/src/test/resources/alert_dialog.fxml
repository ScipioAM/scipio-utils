<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="250.0" prefWidth="400.0" stylesheets="/dialog.css" xmlns="http://javafx.com/javafx/8.0.202" xmlns:fx="http://javafx.com/fxml/1" fx:controller="old_dialogs.AlertDialogController">
    <Label fx:id="label_title" styleClass="dialog-title" text="Label" AnchorPane.leftAnchor="40" AnchorPane.topAnchor="40" />
    <Label fx:id="label_content" styleClass="dialog-content" text="Label" AnchorPane.leftAnchor="40" AnchorPane.topAnchor="100" />
    <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="50.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="20.0">
        <JFXButton fx:id="btn_negative" mnemonicParsing="false" styleClass="dialog-jfx-button" text="取消" />
        <JFXButton fx:id="btn_positive" mnemonicParsing="false" styleClass="dialog-jfx-button" text="确定" />
    </HBox>
</AnchorPane>
